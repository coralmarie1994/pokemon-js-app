{
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "quotes": ["error", "single"],
        "no-unused-vars":"warn",
        "prettier/prettier": "error"
    },
    "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
